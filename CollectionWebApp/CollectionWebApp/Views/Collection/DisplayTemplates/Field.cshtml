@model CollectionWebApp.Models.Field

<div class="field-item">
    <p>Name: @Html.DisplayFor(m => m.Name)</p>
    <p>Type: @Html.DisplayFor(m => m.Type)</p>
    <a asp-action="Edit" asp-controller="Field" asp-route-id="@Model.Id">Edit</a>
    <a asp-action="Delete" asp-controller="Field" asp-route-id="@Model.Id">Delete</a>
</div>